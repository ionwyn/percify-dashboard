{
    "name": "fuse-react-app",
    "version": "1.1.2",
    "private": true,
    "dependencies": {
        "@material-ui/core": "1.2.1",
        "@material-ui/icons": "1.1.0",
        "@material-ui/lab": "^1.0.0-alpha.8",
        "autosuggest-highlight": "3.1.1",
        "axios": "0.18.0",
        "axios-mock-adapter": "1.15.0",
        "babel-polyfill": "6.26.0",
        "chart.js": "2.7.2",
        "dataframe-js": "^1.3.2",
        "downshift": "2.0.0",
        "firebase": "5.0.4",
        "formsy-react": "1.1.4",
        "google-map-react": "1.0.4",
        "husky": "^0.14.3",
        "jss-extend": "6.2.0",
        "keycode": "2.2.0",
        "lint-staged": "^7.2.0",
        "lodash": "4.17.10",
        "mobile-detect": "1.4.2",
        "moment": "2.22.2",
        "path-to-regexp": "2.2.1",
        "perfect-scrollbar": "1.4.0",
        "prettier": "^1.13.5",
        "prismjs": "1.15.0",
        "qs": "6.5.2",
        "raw-loader": "0.5.1",
        "react": "16.4.1",
        "react-autosuggest": "9.3.4",
        "react-big-calendar": "0.19.1",
        "react-chartjs-2": "2.7.2",
        "react-dnd": "2.6.0",
        "react-dnd-html5-backend": "2.6.0",
        "react-dom": "16.4.2",
        "react-number-format": "3.4.0",
        "react-popper": "0.10.4",
        "react-redux": "5.0.7",
        "react-router-config": "1.0.0-beta.4",
        "react-router-dom": "4.3.1",
        "react-scripts": "1.1.4",
        "react-select": "1.2.1",
        "react-swipeable-views": "0.12.13",
        "react-table": "6.8.6",
        "react-text-mask": "5.4.2",
        "redux": "4.0.0",
        "redux-thunk": "2.3.0",
        "spotify-web-api-js": "^0.24.0",
        "typeface-roboto": "0.0.54",
        "typescript": "^2.9.2",
        "velocity-react": "1.4.1"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,json,css}": [
            "prettier --single-quote --write",
            "git add"
        ]
    },
    "scripts": {
        "precommit": "lint-staged",
        "start": "yarn run build-css-helpers && cross-env NODE_PATH=src react-scripts start",
        "build": "yarn run build-css-helpers && cross-env NODE_PATH=src react-scripts build",
        "build-css-helpers": "./node_modules/.bin/tailwind build ./src/styles/fuse-helpers.tailwind.css -c ./tailwind.js -o ./src/styles/fuse-helpers.css && postcss --use autoprefixer -o ./src/styles/fuse-helpers.css ./src/styles/fuse-helpers.css",
        "test": "cross-env NODE_PATH=src react-scripts test --env=jsdom",
        "eject": "cross-env NODE_PATH=src react-scripts eject",
        "build-docs": "babel-node src/main/content/components/material-ui/build.js"
    },
    "devDependencies": {
        "autoprefixer": "8.6.2",
        "babel-preset-env": "1.7.0",
        "cross-env": "5.2.0",
        "js-beautify": "1.7.5",
        "marked": "0.4.0",
        "postcss-cli": "5.0.0",
        "tailwindcss": "0.5.3"
    }
}
