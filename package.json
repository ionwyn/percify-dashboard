    "test": "cross-env NODE_PATH=src react-scripts jest --env=jsdom",
    "test-ci": "react-scripts test --env=jsdom --coverage --testResultsProcessor ./node_modules/jest-teamcity-reporter",
    "eject": "cross-env NODE_PATH=src react-scripts eject",
    "build-docs": "babel-node src/main/content/components/material-ui/build.js",
    "am-apt": "apt moo",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
